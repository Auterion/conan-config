[settings]
os = Linux
arch = x86_64
compiler = clang
compiler.version = 14
build_type = Release

compiler.libcxx = libstdc++11

[buildenv]

[conf]
tools.build:compiler_executables = {'c': '/usr/bin/clang-14', 'cpp': '/usr/bin/clang++-14'}


tools.build:cxxflags += ['-g']
tools.build:cflags += ['-g']
