[settings]
os = Linux
arch = x86_64
compiler = gcc
compiler.version = 9
build_type = Release

compiler.libcxx = libstdc++11

[buildenv]

[conf]
tools.build:compiler_executables = {'c': '/usr/bin/x86_64-linux-gnu-gcc-9', 'cpp': '/usr/bin/x86_64-linux-gnu-g++-9'}


tools.build:cxxflags += ['-g']
tools.build:cflags += ['-g']
